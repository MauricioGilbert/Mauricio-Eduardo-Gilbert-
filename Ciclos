#include "stdafx.h"
#include <iostream>
#include <time.h>
#include <cstdlib>
using namespace std;
	int main () {
		int i, j, k, a;
		int t0;
		for (i = 0; i < 10; i++) {
			for (j = 0; j < 10; j++) {
				for (k = 0; k < 10; k++) {
					for (a = 0; a < 10; a++) {
					cout<<"/r"<< i << j << k << a;
					cout.flush();
					t0 = time (0);
					while (t0 == time(0)) {}
					}
				}
			}
		}
		return 0;
	} 
